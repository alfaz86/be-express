generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Profile {
  id               Int      @id @default(autoincrement())
  image_profil     String?
  image_profil_url String?
  username         String
  name             String
  status           String
  bio              String
  instagram        String
  linkedin         String
  github           String
  createdAt        DateTime @default(now())
}

model Project {
  id          Int    @id @default(autoincrement())
  title       String
  description String
  link        String
  image       String
  url_image   String
}

model Content {
  id           Int    @id @default(autoincrement())
  devTitle     String
  devtools     String
  projectTitle String
  project      String
}
